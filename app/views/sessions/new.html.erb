<h1>Log In</h1>

<%= form_with(scope: :session, url: login_path) do |form| %>

  <div>
    <%= form.label :email, 'Email' %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password, 'Password' %>
    <%= form.password_field :password %>
  </div>
  
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>