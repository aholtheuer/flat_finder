<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Comuna</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% obj.each do |s| %>
      <tr>
        <td><%= s.title %></td>
        <td><%= s.comuna.gsub('-', ' ').titleize() %></td>
        <td><%= link_to 'Show', s %></td>
        <td><%= link_to 'Edit', edit_search_param_path(s) %></td>
        <td><%= link_to 'Destroy', s, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

